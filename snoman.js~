const canvas = document.getElementById("myCanvas1");
    const ctx = canvas.getContext("2d");
function drawCircle(x,y){
        ctx.strokeStyle = "red";
    ctx.lineWidth=10;
    ctx.beginPath();
    ctx.arc(x,y,40,0,2 *  Math.PI);
    ctx.stroke();
}

function arc(x,y){
	ctx.strokeStyle = "red";
    ctx.lineWidth=10;
    ctx.beginPath();
    ctx.arc(x,y,25,0,Math.PI);
    ctx.stroke();

}

drawCircle(100,100);
arc(100,100)

